{
    "name": "idmarinas/message-bundle",
    "type": "symfony-bundle",
    "description": "Provides user-to-user messaging features for your Symfony application. Forked of FOSMessageBundle",
    "keywords": ["symfony", "message", "messaging"],
    "homepage": "https://github.com/idmarinas/MessageBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thibault Duplessis",
            "email": "thibault.duplessis@gmail.com",
            "homepage": "http://ornicar.github.com",
            "role": "Original Creator"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/friendsofsymfony/FOSMessageBundle/contributors",
            "role": "Contributors of original"
        },
        {
            "name": "IvÃ¡n Diaz Marinas (IDMarinas)",
            "email": "35842929+idmarinas@users.noreply.github.com",
            "homepage": "https://github.com/idmarinas/message-bundle",
            "role": "Maintainer of idmarinas/message-bundle Fork"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/config": "^4.4|^5.0",
        "doctrine/collections": "^1.3",
        "symfony/form": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/security": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "nyholm/symfony-bundle-test": "^1.7",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/validator": "^4.4|^5.0",
        "symfony/translation": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/templating": "^4.4|^5.0",
        "symfony/browser-kit": "^4.4|^5.0",
        "doctrine/orm": "^2.0",
        "rector/rector": "^0.10.9"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "dev-master",
        "doctrine/mongodb-odm-bundle": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "FOS\\MessageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FOS\\MessageBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "simple-phpunit"
    }
}
